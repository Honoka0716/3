import java.awt.*;
import javax.swing.*;
public class Win3 extends JFrame {
	public Win3() {
		super("test");
		setSize(800,800);
		Container c = getContentPane();
		setBackground(Color.RED);
		setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		setVisible(true);
	}
	public static void main(String[] args) {
		new Win3();
		new Win3();
	}

}
